<h2>物件一覧</h2>
<table>
<tr>
  <th>物件名</th>
  <th>賃料</th>
  <th>住所</th>
  <th>築年数</th>
  <th>備考</th>
</tr>
<% @properties.each do |property| %>
<tr>
  <td><%= property.property_name %></td>
  <td><%= property.rent %>円</td>
  <td><%= property.address %></td>
  <td><%= property.age %>年</td>
  <td><%= property.remarks %></td>

  <td><%= link_to '詳細を確認する', property_path(property.id) %></td>
</tr>
<% end %>
</table>
